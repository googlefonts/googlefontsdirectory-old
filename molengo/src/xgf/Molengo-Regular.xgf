<?xml version="1.0"?>
<!-- 
    This is the hinting instructions for the TTF version of Molengo-Regular.
-->
<xgridfit xmlns="http://xgridfit.sourceforge.net/Xgridfit2">

  <infile>Molengo-Regular.sfd</infile>
  <outfile>Molengo-Regular.ttf</outfile>
  <!-- Points in the twilight zone should come here -->
  <constant name="left-sidebearing" value="last + 1"/>
  <constant name="right-sidebearing" value="left-sidebearing + 1"/>
  <!--
  Control Value Table
  Control value are named by style-type-value
  Styles for Latin script:
    default - lowercase
    "uc" - uppercase
    "onum" - old style figures
  Types for Latin script:
    default - straight glyphs or parts like 'l', 'n', etc.
    "rnd" - round glyphs or parts like 'o', 'c', etc.
-->
  <control-value name="baseline" value="0"/>
  <control-value name="round-bottom" value="-18"/>
    <!-- cap-round-bottom could be different -->
  <control-value name="cap-round-bottom" value="-18"/>
  <control-value name="straight-bearing" value="164"/>
  <control-value name="cap-straight-bearing" value="180"/>
  <control-value name="round-bearing" value="61"/>
  <control-value name="cap-round-bearing" value="45"/>
    <!-- Lowercase -->
    <!-- vert-curve could be a bit larger than vert-stem -->
  <control-value name="vert-stem" value="161"/>
  <control-value name="vert-curve" value="161"/>
  <control-value name="horiz-stem" value="128"/>
  <control-value name="horiz-curve" value="128"/>
    <!-- Uppercase -->
  <control-value name="cap-vert-stem" value="181"/>
  <control-value name="cap-vert-curve" value="192"/>
  <control-value name="cap-horiz-stem" value="139"/>
  <control-value name="cap-horiz-curve" value="141"/>
    <!-- Old style figures -->
    <!-- onum-round-bottom could be different if necessary-->
    <!-- onum-vert-curve could be a bit larger -->
  <control-value name="onum-round-bearing" value="61"/>
  <control-value name="onum-round-bottom" value="-18"/>
  <control-value name="onum-vert-stem" value="161"/>
  <control-value name="onum-vert-curve" value="161"/>
  <control-value name="onum-horiz-stem" value="128"/>
  <control-value name="onum-horiz-curve" value="128"/>
    <!--
  x: 200/173 - thick, diag
  k: 200 - thick, thick
  v,w,y: 173/162 - diag, thin
  z: 186 - thick-diag
  thinner-diag, where was this?

-->
  <control-value name="thicker-diag" value="200"/>
  <control-value name="thick-diag" value="186"/>
  <control-value name="diag" value="174"/>
  <control-value name="thin-diag" value="162"/>
  <control-value name="thinner-diag" value="114"/>
  <control-value name="cap-n-diag" value="348"/>
  <control-value name="x-height" value="938"/>
  <control-value name="o-height" value="956"/>
  <control-value name="onum-height" value="1049"/>
  <control-value name="onum-round-height" value="1067"/>
  <control-value name="onum-tall-height" value="1305"/>
  <control-value name="onum-desc" value="-256"/>
  <control-value name="onum-round-desc" value="-350"/>
  <control-value name="ascender-height" value="1475"/>
  <control-value name="descender-bottom" value="-492"/>
  <control-value name="f-top" value="1464"/>
    <!-- Capitals -->
  <control-value name="cap-x-height" value="1391"/>
  <control-value name="cap-o-height" value="1409"/>
    <!-- glyph specific -->
  <control-value name="zero-width" value="1110"/>
  <control-value name="eight-width" value="815"/>
  <control-value name="dash-height" value="109"/>
  <control-value name="bar-width" value="109"/>
  <control-value name="dot-width" value="236"/>
  <control-value name="dot-height" value="236"/>
  <control-value name="acute-height" value="375"/>
  <control-value name="cap-o-width" value="1354"/>
  <control-value name="a-width" value="706"/>
  <control-value name="a-vert-diag-stem" value="117"/>
  <control-value name="a-vert-dip" value="90"/>
    <!-- a, f -->
  <control-value name="b-width" value="835"/>
  <control-value name="c-vert-dip" value="100"/>
  <control-value name="e-vert-dip" value="73"/>
  <control-value name="e-middle-bar" value="496"/>
    <!-- vert distance from top -->
  <control-value name="f-stem-advance" value="222"/>
    <!-- f, i, j -->
  <control-value name="f-height" value="1464"/>
  <control-value name="h-width" value="793"/>
    <!-- h, n -->
  <control-value name="k-width" value="749"/>
  <control-value name="m-half-width" value="739"/>
  <control-value name="o-width" value="890"/>
  <control-value name="s-width" value="630"/>
    <!-- from curve to curve, not end to end -->
  <control-value name="v-bearing" value="21"/>
  <control-value name="v-width" value="942"/>
  <control-value name="y-width" value="982"/>

  <pre-program>
    <if test="pixels-per-em &lt; 9 or pixels-per-em &gt; 1024">
      <disable-instructions/>
    </if>
    <round value="round-bottom"/>
    <round value="cap-round-bottom"/>
    <round value="vert-stem"/>
    <round value="vert-curve"/>
    <round value="horiz-stem"/>
    <round value="horiz-curve"/>
    <round value="cap-vert-stem"/>
    <round value="cap-vert-curve"/>
    <round value="cap-horiz-stem"/>
    <round value="cap-horiz-curve"/>
    <control-value-delta>
      <delta-set size="1" distance="8" cv="onum-round-height"/>
      <delta-set size="3" distance="8" cv="onum-round-height"/>
      <delta-set size="5" distance="8" cv="onum-round-height"/>
      <delta-set size="1" distance="8" cv="onum-height"/>
      <delta-set size="3" distance="8" cv="onum-height"/>
      <delta-set size="5" distance="8" cv="onum-height"/>
      <delta-set size="17" distance="8" cv="dash-height"/>
      <delta-set size="18" distance="8" cv="dash-height"/>
      <delta-set size="19" distance="8" cv="dash-height"/>
      <delta-set size="5" distance="8" cv="zero-width"/>
      <delta-set size="5" distance="-8" cv="o-height"/>
      <delta-set size="16" distance="8" cv="x-height"/>
      <delta-set size="18" distance="8" cv="x-height"/>
      <delta-set size="20" distance="8" cv="x-height"/>
      <delta-set size="2" distance="-8" cv="cap-o-height"/>
      <delta-set size="7" distance="-8" cv="cap-vert-stem"/>
      <delta-set size="17" distance="8" cv="cap-vert-stem"/>
      <delta-set size="18" distance="8" cv="cap-vert-stem"/>
      <delta-set size="19" distance="8" cv="cap-vert-stem"/>
      <delta-set size="7" distance="-8" cv="cap-vert-curve"/>
      <delta-set size="17" distance="8" cv="cap-vert-curve"/>
      <delta-set size="13" distance="-8" cv="cap-horiz-stem"/>
      <delta-set size="13" distance="-8" cv="cap-horiz-curve"/>
      <delta-set size="7" distance="-8" cv="thicker-diag"/>
      <delta-set size="8" distance="8" cv="vert-stem"/>
      <delta-set size="9" distance="8" cv="vert-stem"/>
      <delta-set size="17" distance="8" cv="vert-stem"/>
      <delta-set size="18" distance="8" cv="vert-stem"/>
      <delta-set size="19" distance="8" cv="vert-stem"/>
      <delta-set size="20" distance="8" cv="vert-stem"/>
      <delta-set size="21" distance="8" cv="vert-stem"/>
      <delta-set size="22" distance="8" cv="vert-stem"/>
      <delta-set size="8" distance="8" cv="vert-curve"/>
      <delta-set size="9" distance="8" cv="vert-curve"/>
      <delta-set size="17" distance="8" cv="vert-curve"/>
      <delta-set size="18" distance="8" cv="vert-curve"/>
      <delta-set size="19" distance="8" cv="vert-curve"/>
      <delta-set size="20" distance="8" cv="vert-curve"/>
      <delta-set size="21" distance="8" cv="vert-curve"/>
      <delta-set size="22" distance="8" cv="vert-curve"/>
      <delta-set size="8" distance="8" cv="onum-vert-stem"/>
      <delta-set size="9" distance="8" cv="onum-vert-stem"/>
      <delta-set size="17" distance="8" cv="onum-vert-stem"/>
      <delta-set size="18" distance="8" cv="onum-vert-stem"/>
      <delta-set size="19" distance="8" cv="onum-vert-stem"/>
      <delta-set size="20" distance="8" cv="onum-vert-stem"/>
      <delta-set size="21" distance="8" cv="onum-vert-stem"/>
      <delta-set size="22" distance="8" cv="onum-vert-stem"/>
      <delta-set size="8" distance="8" cv="onum-vert-curve"/>
      <delta-set size="9" distance="8" cv="onum-vert-curve"/>
      <delta-set size="17" distance="8" cv="onum-vert-curve"/>
      <delta-set size="18" distance="8" cv="onum-vert-curve"/>
      <delta-set size="19" distance="8" cv="onum-vert-curve"/>
      <delta-set size="20" distance="8" cv="onum-vert-curve"/>
      <delta-set size="21" distance="8" cv="onum-vert-curve"/>
      <delta-set size="22" distance="8" cv="onum-vert-curve"/>
      <delta-set size="7" distance="-8" cv="a-width"/>
      <delta-set size="7" distance="-8" cv="b-width"/>
      <delta-set size="4" distance="-8" cv="dot-width"/>
      <delta-set size="5" distance="-8" cv="dot-width"/>
      <delta-set size="6" distance="-8" cv="dot-width"/>
      <delta-set size="7" distance="-8" cv="dot-width"/>
      <delta-set size="13" distance="-8" cv="dot-width"/>
      <delta-set size="14" distance="-8" cv="dot-width"/>
      <delta-set size="15" distance="-8" cv="dot-width"/>
      <delta-set size="16" distance="-8" cv="dot-width"/>
      <!--
      <delta-set size="9" distance="8" cv="horiz-stem"/>
      <delta-set size="9" distance="8" cv="horiz-curve"/>
-->
    </control-value-delta>
  </pre-program>

  <glyph ps-name="exclam">
      <constant name="top-left" value="0"/>
      <constant name="bottom-left" value="3"/>
      <constant name="top-right" value="1"/>
      <constant name="bottom-right" value="2"/>
      <constant name="dot-left" value="4"/>
      <constant name="dot-top" value="7"/>
      <constant name="dot-right" value="10"/>
      <constant name="dot-bottom" value="13"/>
      <constant name="last" value="11"/>
    <with-vectors axis="y">
      <move distance="round-bottom">
        <point num="dot-bottom"/>
        <move distance="dot-height">
          <point num="dot-top"/>
        </move>
      </move>
      <move distance="cap-x-height">
        <point num="top-left"/>
      </move>
      <move>
        <reference>
          <point num="dot-top"/>
        </reference>
        <point num="bottom-left"/>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="dot-left"/>
        <align>
          <point num="top-left"/>
        </align>
        <move distance="dot-width">
          <point num="dot-right"/>
          <align>
            <point num="top-right"/>
          </align>
        </move>
      </move>
      <if test="pixels-per-em &lt; 17">
        <align>
          <reference>
            <point num="top-right"/>
          </reference>
          <point num="bottom-right"/>
        </align>
        <align>
          <reference>
            <point num="top-left"/>
          </reference>
          <point num="bottom-left"/>
        </align>
      </if>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="quotedbl">
      <constant name="quote-first" value="0"/>
      <constant name="quote-second" value="4"/>
      <constant name="right-sidebearing" value="9"/>
    <with-vectors axis="y">
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="quote-first"/>
        </reference>
        <point num="quote-second + 2"/>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
    <move>
      <reference>
        <point num="quote-second"/>
      </reference>
      <point num="right-sidebearing"/>
    </move>
  </glyph>

  <glyph ps-name="quotesingle">
      <constant name="last" value="3"/>
      <constant name="bottom-left" value="2"/>
      <constant name="bottom-right" value="1"/>
      <constant name="top-left" value="3"/>
      <constant name="top-right" value="0"/>
    <with-vectors axis="y">
      <move>
        <point num="top-left"/>
        <move>
          <point num="bottom-left"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="bottom-left"/>
        <align>
          <point num="top-left"/>
        </align>
        <move>
          <point num="bottom-right"/>
        </move>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="parenleft">
      <constant name="last" value="21"/>
      <constant name="curve-left" value="11"/>
      <constant name="curve-right" value="0"/>
      <constant name="bottom-left" value="6"/>
      <constant name="bottom-right" value="5"/>
      <constant name="top-left" value="16"/>
      <constant name="top-right" value="17"/>
    <with-vectors axis="y">
      <move distance="ascender-height">
        <point num="top-left"/>
      </move>
      <move distance="descender-bottom">
        <point num="bottom-left"/>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="curve-left"/>
        <move distance="vert-curve">
          <point num="curve-right"/>
          <move>
            <point num="right-sidebearing"/>
          </move>
        </move>
        <move>
          <point num="bottom-left"/>
          <align>
            <point num="top-left"/>
          </align>
          <move>
            <point num="bottom-right"/>
            <align>
              <point num="top-right"/>
            </align>
          </move>
        </move>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="parenright">
      <constant name="last" value="21"/>
      <constant name="curve-right" value="11"/>
      <constant name="curve-left" value="0"/>
      <constant name="top-right" value="6"/>
      <constant name="top-left" value="5"/>
      <constant name="bottom-right" value="16"/>
      <constant name="bottom-left" value="17"/>
    <with-vectors axis="y">
      <move distance="ascender-height">
        <point num="top-left"/>
      </move>
      <move distance="descender-bottom">
        <point num="bottom-left"/>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="right-sidebearing"/>
        </reference>
        <point num="curve-right"/>
        <move distance="vert-curve">
          <point num="curve-left"/>
          <move>
            <point num="left-sidebearing"/>
          </move>
        </move>
        <move>
          <point num="bottom-right"/>
          <align>
            <point num="top-right"/>
          </align>
          <move>
            <point num="bottom-left"/>
            <align>
              <point num="top-left"/>
            </align>
          </move>
        </move>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="plus">
      <constant name="left-top" value="8"/>
      <constant name="left-bottom" value="7"/>
      <constant name="top-right" value="11"/>
      <constant name="top-left" value="10"/>
      <constant name="right-bottom" value="2"/>
      <constant name="right-top" value="1"/>
      <constant name="bottom-left" value="5"/>
      <constant name="bottom-right" value="4"/>
      <constant name="last" value="11"/>
    <with-vectors axis="y">
      <mdap>
        <point num="bottom-left"/>
      </mdap>
      <move>
        <point num="right-bottom"/>
        <align>
          <point num="left-bottom"/>
        </align>
        <move distance="dash-height">
          <point num="left-top"/>
          <align>
            <point num="right-top"/>
          </align>
        </move>
        <move>
          <point num="top-left"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="left-top"/>
        <move>
          <point num="top-left"/>
          <align>
            <point num="bottom-left"/>
          </align>
          <move distance="dash-height">
            <point num="top-right"/>
            <align>
              <point num="bottom-right"/>
            </align>
            <move>
              <point num="right-top"/>
              <move>
                <point num="right-sidebearing"/>
              </move>
            </move>
          </move>
        </move>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="comma">
      <constant name="bottom" value="16"/>
      <constant name="bottom-over" value="17"/>
      <constant name="crotch" value="0"/>
      <constant name="top" value="8"/>
      <constant name="left" value="5"/>
      <constant name="right" value="13"/>
      <constant name="last" value="18"/>
    <with-vectors axis="y">
      <move distance="dot-height">
        <point num="top"/>
      </move>
      <move>
        <point num="bottom"/>
        <move min-distance="no" round="no">
          <point num="bottom-over"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="left"/>
        <move distance="dot-width">
          <point num="right"/>
          <delta>
            <delta-set size="4" distance="-8"/>
            <delta-set size="5" distance="-8"/>
            <delta-set size="6" distance="-8"/>
            <delta-set size="7" distance="-8"/>
          </delta>
          <move>
            <point num="right-sidebearing"/>
          </move>
        </move>
      </move>
      <interpolate>
        <reference>
          <point num="right"/>
          <point num="left"/>
        </reference>
        <point num="crotch"/>
      </interpolate>
      <if test="pixels-per-em &lt; 64">
        <move>
          <reference>
            <point num="right"/>
          </reference>
          <point num="bottom"/>
				</move>
        <if test="pixels-per-em &lt; 16">
          <move>
              <point num="bottom-over"/>
          </move>
        </if>
      </if>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="hyphen">
      <constant name="left-top" value="2"/>
      <constant name="right-bottom" value="0"/>
      <constant name="last" value="3"/>
    <with-vectors axis="y">
      <move>
        <point num="right-bottom"/>
        <move distance="dash-height">
          <point num="left-top"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="left-top"/>
        <move>
          <point num="right-bottom"/>
          <move>
            <point num="right-sidebearing"/>
          </move>
        </move>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

<!-- hypen U+2010 -->
  <glyph ps-name="uni2010">
      <constant name="left-top" value="2"/>
      <constant name="right-bottom" value="0"/>
      <constant name="last" value="3"/>
    <with-vectors axis="y">
      <move>
        <point num="right-bottom"/>
        <move distance="dash-height">
          <point num="left-top"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="left-top"/>
        <move>
          <point num="right-bottom"/>
          <move>
            <point num="right-sidebearing"/>
          </move>
        </move>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

<!-- em dash U+2014 -->
  <glyph ps-name="emdash">
      <constant name="left-top" value="2"/>
      <constant name="right-bottom" value="0"/>
      <constant name="last" value="3"/>
    <with-vectors axis="y">
      <move>
        <point num="right-bottom"/>
        <move distance="dash-height">
          <point num="left-top"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="left-top"/>
        <move>
          <point num="right-bottom"/>
          <move>
            <point num="right-sidebearing"/>
          </move>
        </move>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="period">
      <constant name="bottom" value="9"/>
      <constant name="top" value="3"/>
      <constant name="left" value="0"/>
      <constant name="right" value="6"/>
      <constant name="last" value="11"/>
    <with-vectors axis="y">
      <move distance="round-bottom">
        <point num="bottom"/>
        <move distance="dot-height">
          <point num="top"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="left"/>
        <move distance="dot-width">
          <point num="right"/>
          <move>
            <point num="right-sidebearing"/>
          </move>
        </move>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="slash">
      <constant name="top-left" value="0"/>
      <constant name="top-right" value="1"/>
      <constant name="base-left" value="3"/>
      <constant name="base-right" value="2"/>
      <constant name="last" value="3"/>
    <with-vectors axis="y">
      <move distance="baseline">
        <point num="base-left"/>
        <move distance="ascender-height">
          <point num="top-right"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="base-left"/>
        <move>
          <point num="base-right"/>
        </move>
        <move>
          <point num="top-left"/>
          <move>
            <point num="top-right"/>
            <move>
              <point num="right-sidebearing"/>
            </move>
          </move>
        </move>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="zero">
      <constant name="last" value="47"/>
      <constant name="base" value="36"/>
      <constant name="base-over" value="0"/>
      <constant name="top" value="24"/>
      <constant name="top-under" value="12"/>
      <constant name="left-stem-left" value="42"/>
      <constant name="left-stem-right" value="18"/>
      <constant name="right-stem-right" value="30"/>
      <constant name="right-stem-left" value="6"/>
    <with-vectors axis="y">
      <move distance="onum-round-bottom" round="no" cut-in="no">
        <point num="base"/>
        <move distance="onum-horiz-curve">
          <point num="base-over"/>
        </move>
      </move>
      <move distance="onum-round-height">
        <point num="top"/>
        <move distance="onum-horiz-curve">
          <point num="top-under"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move distance="onum-round-bearing" min-distance="no">
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="left-stem-left"/>
